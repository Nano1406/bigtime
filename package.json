{
  "name": "sync-time",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "cra-append-sw": "^2.6.1",
    "eva-icons": "^1.1.1",
    "firebase": "^6.0.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-paypal-button-v2": "^2.3.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "react-service-worker": "^0.1.1",
    "react-with-firebase-auth": "^1.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && cra-append-sw -s ./src/custom-sw.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean-map": "rimraf build/**/*.map",
    "deploy": "npm run build && npm run clean-map && cd build && surge -d https://bigtime.surge.sh",
    "deploy-q": "npm run build && npm run clean-map && cd build && surge -d https://qbigtime.surge.sh",
    "deploy-sm": "npm run build && cd build && surge -d https://qbigtime.surge.sh"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
